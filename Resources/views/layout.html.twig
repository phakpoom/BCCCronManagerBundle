<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>{{ 'Cron Manager' | trans({}, 'BCCCronManagerBundle') }}</title>
    <meta name="description" content="">

    <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js"></script>
    <link href="{{ asset('bundles/bcccronmanager/css/main.css') }}" type="text/css" rel="stylesheet" />
</head>

<body>
<div class="container my-5">
    {% for flashMessage in app.session.flashbag.get('message') %}
          <div class="alert alert-info">{{ flashMessage }}</div>
    {% endfor %}

    {% for flashMessage in app.session.flashbag.get('error') %}
          <div class="alert alert-danger">{{ flashMessage }}</div>
    {% endfor %}

    {% block content %}{% endblock %}
</div>
<script src="{{ asset('bundles/bcccronmanager/js/script.js') }}"></script>
</body>
</html>
